{
    "compilerOptions": {
        "target": "es2022",
        "useDefineForClassFields": false,
        "forceConsistentCasingInFileNames": true,
        "strict": true,
        "noImplicitOverride": true,
        "noPropertyAccessFromIndexSignature": true,
        "noImplicitReturns": true,
        "noFallthroughCasesInSwitch": true,
        "paths": {
            "@radix-ng/primitives/*": [
                "./dist/primitives/*"
            ],
            "@radix-ng/components/avatar": [
                "packages/components/avatar/index.ts"
            ],
            "@radix-ng/components/code": [
                "packages/components/code/index.ts"
            ],
            "@radix-ng/components/kbd": [
                "packages/components/kbd/index.ts"
            ],
            "@radix-ng/components/theme": [
                "packages/components/theme/index.ts"
            ],
            "@radix-ng/components/types": [
                "packages/components/types/index.ts"
            ]
        }
    },
    "files": [],
    "include": [],
    "references": [
        {
            "path": "./tsconfig.lib.json"
        },
        {
            "path": "./tsconfig.spec.json"
        }
    ],
    "extends": "../../tsconfig.base.json",
    "angularCompilerOptions": {
        "enableI18nLegacyMessageIdFormat": false,
        "strictInjectionParameters": true,
        "strictInputAccessModifiers": true,
        "strictTemplates": true
    }
}
