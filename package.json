{
    "name": "radix-angular",
    "version": "0.0.0",
    "license": "MIT",
    "private": true,
    "engines": {
        "node": "^20.9"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/radix-ng/primitives.git"
    },
    "packageManager": "pnpm@9.15.0",
    "scripts": {
        "----Storybook----": "-----------------------------------------------------------------------",
        "storybook:primitives": "nx run storybook-radix:storybook",
        "storybook:components": "nx run components:storybook",
        "build-storybook": "nx run storybook-radix:build-storybook",
        "----showcases----------": "-----------------------------------------------------------------",
        "showcase-taxonomy:serve": "nx serve showcase-taxonomy",
        "showcase-taxonomy:build": "nx build showcase-taxonomy",
        "showcase-dashboard:serve": "nx serve showcase-dashboards",
        "showcase-dashboard:build": "nx build showcase-dashboards",
        "----Radix Primitives---": "-----------------------------------------------------------------",
        "primitives:build": "nx run-many --targets=build,compodoc --projects=primitives",
        "primitives:test": "nx run primitives:test",
        "primitives:ssr:serve": "nx serve radix-ssr-testing",
        "primitives:build:typedoc": "node tools/scripts/api-doc/build.mjs",
        "----Radix Docs---": "-----------------------------------------------------------------------",
        "radix-docs:dev": "nx run radix-docs:dev",
        "radix-docs:build": "nx run radix-docs:build",
        "----Radix Components---": "-----------------------------------------------------------------",
        "components:build": "nx run-many --targets=build,compodoc --projects=components",
        "----shadcn-------": "-----------------------------------------------------------------------",
        "shadcn:docs:serve": "nx run shadcn-docs:serve",
        "shadcn:docs:build": "nx run shadcn-docs:build",
        "shadcn:build": "nx run shadcn:build",
        "----Others-------": "-----------------------------------------------------------------------",
        "clean:node-modules": "rimraf ./apps/**/node_modules && rimraf ./packages/**/**/node_modules && rimraf ./node_modules",
        "prepare": "husky",
        "prettier": "prettier '**/*.{css,scss,md,mdx,yml,json,js,cjs,mjs,html,ts,astro}' --check",
        "prettier:fix": "pnpm run prettier --write",
        "stylelint": "stylelint '**/*.{css,scss}'",
        "stylelint:fix": "pnpm run stylelint --fix",
        "eslint": "eslint . --ext='.js,.cjs,.mjs,.ts,.html'",
        "eslint:fix": "pnpm run eslint --fix",
        "----NX-------": "-----------------------------------------------------------------------",
        "format": "nx format --base head~1",
        "migrate": "nx migrate latest",
        "run-migrate": "nx migrate --run-migrations --if-exists",
        "help": "nx help",
        "release:dry-run": "nx release --dry-run -- --skip-nx-cache",
        "----RELEASE Scripts-------": "---------------------------------------------------------",
        "release:skip-publish": "nx release --skip-publish",
        "release:primitives": "nx release --projects=primitives --skip-publish",
        "release:shadcn": "nx release --projects=shadcn --skip-publish"
    },
